  "main": "client/app.js",
  "scripts": {
    "lint": "eslint client",
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "start": "npm run webpack:dev",
    "start:prod": "npm run build:prod && npm run server:prod",
    "webpack:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base client/",
    "server:prod": "http-server dist --cors -o -c-1 -p 3000",
		"server:cv": "http-server cv --cors -o -c-1 -p 3002"
  },
